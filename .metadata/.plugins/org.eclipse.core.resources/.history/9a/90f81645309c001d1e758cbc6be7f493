	public static int leggiVoto() {
		
		Scanner sc = new Scanner(System.in);
		int voto;
		
		do {
			voto=sc.nextInt();
			
			if (voto <= 18 || voto >=30)
				System.out.println("Il numero che hai inserito Ã¨ fuori dal range");
			
		} while (voto <= 18 || voto >=30);
		
		return voto;
		
	}
	
	
	
