import java.util.Scanner;

public class voti {

	public static void main(String[] args) {
		
		int valore=leggiVoto();
		double med=valore;
		
		
		for (int i=2; i<=10; i++) {  //i=2 perché cosi posso fare la media a partire da 2 numeri
			valore=leggiVoto();
			med=media(med, valore, i);
		}
		
		System.out.println(med);
		
	}

	public static double media(int val, double media, int i) {
		
		media=(media+val)/i;		
			
		return media;
	}	
	
	public static int leggiVoto() {
		
		Scanner sc = new Scanner(System.in);
		int voto;
		
		do {
			voto=sc.nextInt();
			
			if (voto <= 18 || voto >=30)
				System.out.println("Il numero che hai inserito è fuori dal range");
			
		} while (voto <= 18 || voto >=30);
		
		return voto;
		
	}
	
}
